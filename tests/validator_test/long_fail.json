{
  "project_metadata": {
    "name": "DevOpsToolbox-Extreme-Test",
    "version": "1.0.0",
    "environment": "production",
    "tags": ["cli", "python", "testing", "validation"]
  },
  "clusters": [
    {
      "name": "us-east-1",
      "nodes": 50,
      "provider": "AWS",
      "config": {
        "instance_type": "t3.medium",
        "auto_scale": true
      }
    },
    {
      "name": "eu-central-1",
      "nodes": 25,
      "provider": "Azure",
      "config": {
        "instance_type": "Standard_D2s_v3",
        "auto_scale": false
      }
    }
  ],
  "monitoring": {
    "enabled": true,
    "interval": "30s",
    "alerts": {
      "cpu_threshold": 80,
      "memory_threshold": 90,
      "slack_channel": "#ops-alerts"
    }
  },
  "database": {
    "engine": "postgres",
    "version": "15.4",
    "replicas": 3,
    "backup": {
      "enabled": true,
      "schedule": "0 2 * * *",
      "retention_days": 7
    }
  },
  "security": {
    "firewall": "enabled",
    "vpn_required": true,
    "allowed_ips": [
      "192.168.1.1",
      "10.0.0.1",
      "172.16.0.1"
    ]
  },
  "microservices": [
    {"name": "auth-service", "port": 8080, "health_check": "/health"},
    {"name": "payment-service", "port": 8081, "health_check": "/ready"},
    {"name": "inventory-service", "port": 8082, "health_check": "/status"},
    {"name": "shipping-service", "port": 8083, "health_check": "/health"},
    {"name": "email-service", "port": 8084, "health_check": "/health"},
    {"name": "frontend-service", "port": 3000, "health_check": "/"}
  ],
  "storage": {
    "bucket_name": "production-assets-bucket",
    "region": "us-west-2",
    "policy": "private"
  },
  "developer_team": {
    "lead": "James",
    "members": [
      "Alice",
      "Bob",
      "Charlie",
      "Dave",
      "Eve"
    ]
  },
  "maintenance": {
    "window": "Sunday 04:00 UTC",
    "contact": "admin@example.com"
  },
  "api_keys": {
    "stripe": "sk_test_123",
    "sendgrid": "sg_456",
    "github": "ghp_789"
  },
  "logging": {
    "level": "info",
    "format": "json",
    "destination": "/var/log/app.log"
  },
  "misc_settings": {
    "cache_enabled": true
    "debug_mode": false
  }
}